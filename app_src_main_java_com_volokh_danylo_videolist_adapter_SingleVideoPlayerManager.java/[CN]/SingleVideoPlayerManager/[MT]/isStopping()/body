{
  boolean result=false;
switch (mCurrentPlayerState.get()) {
case IDLE:
    break;
case INITIALIZED:
case PREPARING:
case PREPARED:
case STARTING:
case STARTED:
case PAUSING:
case PAUSED:
case STOPPED:
case PLAYBACK_COMPLETED:
case END:
case ERROR:
  result=false;
break;
case STOPPING:
result=true;
break;
}
return result;
}

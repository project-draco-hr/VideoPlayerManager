{
  boolean result=false;
switch (mCurrentPlayerState.get()) {
case IDLE:
    break;
case INITIALIZED:
case PREPARING:
case PREPARED:
case STARTING:
case STARTED:
  result=true;
break;
case PAUSING:
case PAUSED:
case STOPPING:
case STOPPED:
case PLAYBACK_COMPLETED:
case END:
case ERROR:
result=false;
break;
}
return result;
}
